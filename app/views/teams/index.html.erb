



<% form_tag teams_path, :method => 'get' do %>
  <p>
  <%= text_field_tag :search, params[:search] %> 
  <%= submit_tag 'Search', :name => nil %>
  </p>
<% end %>

<table id="teams-table">
  <% @teams.each do |team| %>
  <tr>
    <td><%= team.name %></td>
    <td><%= team.state %></td>
    <td><%= team.team_avg %></td>
  </tr>
  <% end %>
</table>
<br />

<%= button_to 'Add Your Team', new_team_path %>