<h2>Add Your Team</h2>

<% form_for @team do |f| %>
  <%= f.error_messages %>
  First Name: <%= f.text_field :name %><br />
  League: <%= f.text_field :league %><br />
  Section: <%= f.text_field :section %><br />
  County: <%= f.text_field :county %><br />
  State: <%= f.text_field :state %><br />
  Country: <%= f.text_field :country %><br /> 
  <%= f.submit 'Add Team' %>
<% end %>
